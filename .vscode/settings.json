{
  "files.eol": "\n",
  "files.exclude": {
    "**/node_modules": false,
    "**/.tsconfig.*": false
  },
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/build": true
  },
  "eslint.debug": true,
  "eslint.lintTask.enable": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.codeActionsOnSave.mode": "problems",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.words": [
    "appbar",
    "Arie",
    "autocrlf",
    "boundwitness",
    "bowser",
    "chartjs",
    "chromaui",
    "cytoscape",
    "darkmode",
    "DATETIME",
    "ellipsize",
    "etherchain",
    "ethersproject",
    "ethgasstation",
    "flexbox",
    "Hasher",
    "huri",
    "instrumenter",
    "mapbox",
    "pako",
    "payloadset",
    "Promisable",
    "quadkey",
    "rollbar",
    "scure",
    "secp",
    "Trouw",
    "typechain",
    "wordlist",
    "wordlists",
    "xylabs",
    "yarnpkg"
  ],
  "npm.packageManager": "yarn"
}
