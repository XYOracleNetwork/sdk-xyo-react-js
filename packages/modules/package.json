{
  "name": "@xyo-network/react-modules",
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js/issues"
  },
  "dependencies": {
    "@xyo-network/react-archivist": "workspace:^",
    "@xyo-network/react-diviner": "workspace:^",
    "@xyo-network/react-module": "workspace:^",
    "@xyo-network/react-node": "workspace:^",
    "@xyo-network/react-sentinel": "workspace:^",
    "@xyo-network/react-witness": "workspace:^"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.15.16",
    "@xylabs/tsconfig-react": "^3.15.16",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@mui/icons-material": "^5",
    "@mui/material": "^5",
    "@mui/styles": "^5",
    "geojson": "^0.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4"
  },
  "description": "Common React library for all XYO projects that use React",
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        },
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.cjs"
        }
      },
      "browser": {
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.mjs"
        },
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        }
      },
      "default": "./dist/browser/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/browser/index.cjs",
  "module": "dist/browser/index.mjs",
  "homepage": "https://xyo.network",
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "react"
  ],
  "license": "LGPL-3.0-only",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js.git"
  },
  "scripts": {
    "lint-pkg": "npmPkgJsonLint .",
    "license": "yarn license-checker --exclude \"MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0\""
  },
  "sideEffects": false,
  "types": "dist/browser/index.d.ts",
  "version": "2.81.10",
  "type": "module"
}
