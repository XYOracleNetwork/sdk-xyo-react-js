  "files": [
    "dist",
    "src"
  ],
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "build": "xy build",
    "build-storybook": "storybook build && copyfiles -u 1 storybook-static/**/* docs/storybook",
    "build-storybook-debug": "storybook build --debug-webpack",
    "build-typedoc-site": "typedoc",
    "start": "storybook dev -p 6006"
