    "@xylabs/tsconfig-react": "~7.1.7",
    "@xyo-network/hash": "~5.1.2",
    "chromatic": "~13.2.0",
    "copyfiles": "~2.4.1",
    "dotenv": "~17.2.2",
    "eslint": "~9.36.0",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-plugin-storybook": "9.1.7",
    "knip": "~5.64.0",
    "luxon": "~3.7.2",
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "react-helmet": "~6.1.0",
    "react-is": "~19.1.1",
    "react-router-dom": "^7",
    "rimraf": "~6.0.1",
    "rollup": "~4.50.2",
    "storybook": "~9.1.7",
    "ts-api-utils": "~2.1.0",
    "typedoc": "~0.28.13",
    "typescript": "~5.9.2",
    "vite": "~7.1.7",
    "vite-plugin-checker": "~0.10.3",
