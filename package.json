{
  "name": "@xyo-network/sdk-xyo-react-js",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Common React library for all XYO projects that use React",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0",
  "private": false,
  "version": "2.0.31",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-react-js.git"
  },
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./index.d.js",
  "exports": {
    ".": "./index.esm.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -r -f dist && yarn deps && yarn lint && yarn compile && yarn copy-src",
    "copy-src": "cp package.json ./dist && cp -R ./src/* ./dist",
    "compile": "rollup -c rollup.config.ts",
    "deploy": "yarn version --patch && yarn build && yarn publish ./dist --access public",
    "lint": "eslint --ext .ts,.tsx ./src",
    "fix": "eslint --ext .ts,.tsx ./src --fix",
    "test": "env TS_NODE_PROJECT=\"tsconfig.test.json\" mocha -r ts-node/register 'src/**/*.spec.ts'",
    "clean": "rm -r -f dist && rm -f .eslintcache && rm -f yarn.lock && rm -f *.tsbuildinfo",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "tsprune": "yarn ts-prune",
    "deps": "yarn depcheck --ignores \"vue, ts-node, @types/jest, jest, ts-jest\""
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-alpha.30",
    "@xyo-network/sdk-xyo-js": "^2.1.6",
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "query-string": "^7.0.0",
    "randombytes": "^2.1.0",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.2.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.108",
    "@types/numeral": "^2.0.1",
    "@types/randombytes": "^2.0.0",
    "@types/react": "^17.0.2",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "depcheck": "^1.4.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "rollup": "^2.45.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "~4.2.2",
    "vue": "^3.0.11"
  }
}
