    "@xylabs/eslint-config-flat": "~7.1.7",
    "@xylabs/eslint-config-react-flat": "~7.1.7",
    "@xylabs/react-invertible-theme": "~7.1.0",
    "@xylabs/react-theme": "~7.1.0",
